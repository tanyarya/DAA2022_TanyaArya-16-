#include<stdio.h>
int main()
{
    int a[10],n,key,count=0,flag=0;
    printf("enter length of array\n");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter key to be searched\n");
    scanf("%d",&key);
    for(int i=0;i<n;i++)
    {
        count++;
        if(a[i]==key)
        {
            printf("key found at %d",count);
            flag++;
        }
    }
    if(flag==0)
    printf("key not found");
}
